{
    "name": "recca0120/config",
    "description": "Laravel 5 Config",
    "keywords": ["laravel", "config", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "^5.0",
        "illuminate/database": "^5.0",
        "illuminate/filesystem": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4|~1.0",
        "phpunit/phpunit": "~4.8|~5.4|~6.1|~7.0",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\Config\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
